local hue = 0
RunService.Heartbeat:Connect(function()
    hue = (hue + 0.005) % 1
    TitleBar.BackgroundColor3 = Color3.fromHSV(hue, 0.8, 0.8)
end)

-- æ·»åŠ ç‰ˆæœ¬ä¿¡æ¯
